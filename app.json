    "LANG": {
      "required": true
    },
    "NEW_RELIC_API_KEY": {
      "required": true
    },
    "PAGER_DUTY_API_KEY": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "REDIS_PROVIDER": {
      "required": true
    },
    "REDIS_URL": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
