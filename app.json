{
  "name": "g5-app-wrangler-test",
  "scripts": {
    "postdeploy": "bundle exec rake db:setup"
  },
  "env": {
    "CLS_GITHUB_TOKEN": {
      "required": true
    },
    "DSH_GITHUB_TOKEN": {
      "required": true
    },
    "FURY_AUTH": {
      "required": true
    },
    "G5_AUTH_CLIENT_ID": {
      "required": true
    },
    "G5_AUTH_CLIENT_SECRET": {
      "required": true
    },
    "G5_AUTH_ENDPOINT": {
      "required": true
    },
    "G5_AUTH_REDIRECT_URI": {
      "required": true
    },
    "HEROKU_AUTH_TOKEN": {
      "required": true
